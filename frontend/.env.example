# ==============================================
# RabbitFun Launchpad - Frontend Environment Variables
# ==============================================
# Copy this file to .env.local for development
# Copy this file to .env.production for production

# ---------- API Configuration ----------
# Backend API URL
VITE_API_BASE_URL=http://localhost:3001
VITE_API_VERSION=v1

# WebSocket connection
VITE_WS_URL=ws://localhost:3001
VITE_WS_RECONNECT_ATTEMPTS=5
VITE_WS_RECONNECT_DELAY=1000

# ---------- Blockchain Configuration ----------
# Network Configuration
VITE_BLOCKCHAIN_NETWORK=bsc
VITE_CHAIN_ID=56
VITE_RPC_URL=https://bsc-dataseed.binance.org
VITE_BLOCK_EXPLORER_URL=https://bscscan.com

# Smart Contract Addresses
VITE_BONDING_CURVE_ADDRESS=0x0000000000000000000000000000000000000000
VITE_TOKEN_FACTORY_ADDRESS=0x0000000000000000000000000000000000000000
VITE_ROUTER_ADDRESS=0x10ED43C718714eb63d5aA57B78B54704E256024E

# ---------- Application Configuration ----------
# App Metadata
VITE_APP_NAME=RabbitFun Launchpad
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Decentralized token launchpad on BSC
VITE_APP_URL=https://rabbitfun.io

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_REPORTING=false
VITE_ENABLE_PERFORMANCE_MONITORING=false
VITE_ENABLE_SOCIAL_FEATURES=false
VITE_ENABLE_ADVANCED_SEARCH=false
VITE_ENABLE_NOTIFICATIONS=false

# Development Features
VITE_ENABLE_DEV_TOOLS=false
VITE_ENABLE_MOCK_DATA=true
VITE_ENABLE_DEBUG_LOGS=false

# ---------- Security Configuration ----------
# CORS Configuration
VITE_CORS_ORIGIN=http://localhost:5173
VITE_CORS_CREDENTIALS=true

# Rate Limiting
VITE_RATE_LIMIT_REQUESTS=100
VITE_RATE_LIMIT_WINDOW=60000

# Content Security Policy
VITE_CSP_ENABLED=true
VITE_CSP_SCRIPT_SRC=self
VITE_CSP_STYLE_SRC=self 'unsafe-inline'
VITE_CSP_IMG_SRC=self data: https:
VITE_CSP_CONNECT_SRC=self https: wss:

# ---------- Performance Configuration ----------
# Caching Configuration
VITE_CACHE_ENABLED=true
VITE_CACHE_TTL=300000
VITE_CACHE_MAX_SIZE=100

# Lazy Loading Configuration
VITE_LAZY_LOADING_ENABLED=true
VITE_LAZY_LOADING_THRESHOLD=200
VITE_PLACEHOLDER_BLUR=true

# Image Optimization
VITE_IMAGE_OPTIMIZATION=true
VITE_IMAGE_QUALITY=80
VITE_IMAGE_PLACEHOLDER=true

# ---------- Analytics & Monitoring ----------
# Google Analytics (Optional)
# VITE_GA_TRACKING_ID=G-XXXXXXXXXX

# Sentry Error Tracking (Optional)
# VITE_SENTRY_DSN=https://xxxxxxxxxx.ingest.sentry.io/xxxxx
# VITE_SENTRY_ENVIRONMENT=production

# Custom Analytics (Optional)
# VITE_ANALYTICS_ENDPOINT=https://analytics.rabbitfun.io/api/events
# VITE_ANALYTICS_API_KEY=your-api-key-here

# ---------- Third-party Services ----------
# Wallet Connection
VITE_WALLETCONNECT_PROJECT_ID=your-project-id-here
VITE_META_MASK_DEEPLINK=https://metamask.app/dapp/

# IPFS Configuration
VITE_IPFS_GATEWAY=https://ipfs.io/ipfs/
VITE_IPFS_BACKUP_GATEWAY=https://gateway.pinata.cloud/ipfs/

# Social Media Integration
VITE_TWITTER_HANDLE=rabbitfun_io
VITE_TELEGRAM_GROUP=https://t.me/rabbitfun
VITE_DISCORD_INVITE=https://discord.gg/rabbitfun

# ---------- UI/UX Configuration ----------
# Theme Configuration
VITE_DEFAULT_THEME=dark
VITE_THEME_PERSISTENCE=true
VITE_ANIMATION_DURATION=300

# Pagination Configuration
VITE_PAGINATION_DEFAULT_SIZE=20
VITE_PAGINATION_MAX_SIZE=100

# Search Configuration
VITE_SEARCH_DEBOUNCE_DELAY=300
VITE_SEARCH_MIN_LENGTH=2
VITE_SEARCH_MAX_RESULTS=50

# Notification Configuration
VITE_NOTIFICATION_DURATION=5000
VITE_NOTIFICATION_POSITION=top-right
VITE_NOTIFICATION_MAX_VISIBLE=5

# ---------- Development & Debugging ----------
# Log Levels: error, warn, info, debug
VITE_LOG_LEVEL=warn
VITE_LOG_TO_CONSOLE=false
VITE_LOG_TO_FILE=false

# Performance Monitoring
VITE_PERFORMANCE_MONITORING=false
VITE_PERFORMANCE_SAMPLE_RATE=0.1

# Feature Development
VITE_ENABLE_BETA_FEATURES=false
VITE_ENABLE_EXPERIMENTAL_FEATURES=false

# ---------- Production Specific ----------
# Build Configuration
VITE_BUILD_SOURCEMAP=false
VITE_BUILD_MINIFY=true
VITE_BUILD_TARGET=es2020

# Security Headers
VITE_SECURITY_HEADERS_ENABLED=true
VITE_X_FRAME_OPTIONS=DENY
VITE_X_CONTENT_TYPE_OPTIONS=nosniff
VITE_REFERRER_POLICY=strict-origin-when-cross-origin

# API Keys (for production - replace with actual values)
# VITE_ETHERSCAN_API_KEY=your-etherscan-api-key
# VITE_COINGECKO_API_KEY=your-coingecko-api-key
# VITE_MORALIS_API_KEY=your-moralis-api-key

# ---------- Testing Configuration ----------
# Testing Environment
VITE_TEST_MODE=false
VITE_TEST_NETWORK=localhost
VITE_TEST_PRIVATE_KEY=your-test-private-key

# Mock Configuration
VITE_ENABLE_MOCK_API=false
VITE_MOCK_API_DELAY=500
VITE_MOCK_ERROR_RATE=0

# E2E Testing
VITE_E2E_TESTING=false
VITE_E2E_HEADLESS=true
VITE_E2E_TIMEOUT=10000

# ---------- Backup & Redundancy ----------
# Backup API Endpoints
VITE_BACKUP_API_URL=https://api-backup.rabbitfun.io
VITE_FAILOVER_ENABLED=false
VITE_FAILOVER_TIMEOUT=5000

# CDN Configuration
VITE_CDN_ENABLED=false
VITE_CDN_URL=https://cdn.rabbitfun.io
VITE_CDN_VERSION=1.0.0

# ---------- Regional Configuration ----------
# Geographic Settings
VITE_DEFAULT_REGION=global
VITE_ENABLE_REGIONAL_FEATURES=false
VITE_REGION_DETECTION=true

# Currency Configuration
VITE_DEFAULT_CURRENCY=USD
VITE_ENABLE_CURRENCY_CONVERSION=false
VITE_CURRENCY_API_KEY=your-currency-api-key

# Language Configuration
VITE_DEFAULT_LANGUAGE=en
VITE_ENABLE_MULTILINGUAL=false
VITE_SUPPORTED_LANGUAGES=en,es,fr,de,ja,ko,zh

# ---------- Legal & Compliance ----------
# Privacy Policy
VITE_PRIVACY_POLICY_URL=https://rabbitfun.io/privacy
VITE_TERMS_OF_SERVICE_URL=https://rabbitfun.io/terms

# Age Restriction
VITE_AGE_RESTICTION_ENABLED=false
VITE_MINIMUM_AGE=18

# Regulatory Compliance
VITE_GDPR_COMPLIANCE=true
VITE_COOKIE_CONSENT_ENABLED=false
VITE_DATA_RETENTION_DAYS=365

# ---------- Monitoring & Alerts ----------
# Health Check Configuration
VITE_HEALTH_CHECK_ENABLED=true
VITE_HEALTH_CHECK_INTERVAL=30000
VITE_HEALTH_CHECK_ENDPOINT=/health

# Error Reporting
VITE_ERROR_REPORTING_ENABLED=false
VITE_ERROR_SAMPLE_RATE=0.1
VITE_ERROR_INCLUDE_USER_DATA=false

# Performance Alerts
VITE_PERFORMANCE_ALERTS_ENABLED=false
VITE_PERFORMANCE_THRESHOLD=3000
VITE_PERFORMANCE_SAMPLE_RATE=0.05

# ---------- Deployment Configuration ----------
# Environment Detection
VITE_NODE_ENV=production
VITE_DEPLOYMENT_ENV=production
VITE_BUILD_TIMESTAMP=true

# Deployment Metadata
VITE_DEPLOYMENT_VERSION=1.0.0
VITE_DEPLOYMENT_COMMIT_HASH=unknown
VITE_DEPLOYMENT_BRANCH=main

# CDN Deployment
VITE_CDN_DEPLOYMENT=false
VITE_CDN_PROVIDER=cloudflare
VITE_CDN_REGION=global

# ---------- Emergency Controls ----------
# Maintenance Mode
VITE_MAINTENANCE_MODE=false
VITE_MAINTENANCE_MESSAGE=Under maintenance. Please check back soon.
VITE_MAINTENANCE_REDIRECT_URL=https://status.rabbitfun.io

# Emergency Shutdown
VITE_EMERGENCY_SHUTDOWN=false
VITE_EMERGENCY_MESSAGE=Service temporarily unavailable.
VITE_EMERGENCY_CONTACT=support@rabbitfun.io

# ---------- Experimental Features ----------
# Beta Features (disable in production)
VITE_ENABLE_BETA_UI=false
VITE_ENABLE_BETA_API=false
VITE_ENABLE_BETA_ANALYTICS=false

# AI/ML Features
VITE_ENABLE_AI_FEATURES=false
VITE_AI_MODEL_VERSION=1.0
VITE_AI_ENDPOINT=https://ai.rabbitfun.io/api

# Advanced Features
VITE_ENABLE_WEB3_MODAL=false
VITE_ENABLE_NFT_FEATURES=false
VITE_ENABLE_STAKING=false

# ---------- Developer Notes ----------
#
# SECURITY INSTRUCTIONS:
# 1. Never commit actual .env files to version control
# 2. Use different values for development and production
# 3. Rotate API keys regularly
# 4. Use environment-specific secrets management
# 5. Enable security headers in production
# 6. Monitor for leaked credentials
#
# DEPLOYMENT INSTRUCTIONS:
# 1. Copy .env.example to .env.production
# 2. Replace placeholder values with actual production values
# 3. Test all API connections
# 4. Verify security headers are working
# 5. Monitor performance and error rates
#
# TROUBLESHOOTING:
# - Check network connectivity for blockchain RPC URLs
# - Verify API endpoints are accessible
# - Test wallet connections with different providers
# - Monitor console for configuration errors
# - Validate feature flags are working correctly