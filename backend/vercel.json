{
  "version": 2,
  "builds": [
    {
      "src": "api/health.ts",
      "use": "@vercel/node"
    },
    {
      "src": "api/tokens.ts",
      "use": "@vercel/node"
    },
    {
      "src": "api/auth.ts",
      "use": "@vercel/node"
    },
    {
      "src": "api/blockchain.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/health",
      "dest": "api/health.ts"
    },
    {
      "src": "/api/tokens",
      "dest": "api/tokens.ts"
    },
    {
      "src": "/api/auth",
      "dest": "api/auth.ts"
    },
    {
      "src": "/api/blockchain",
      "dest": "api/blockchain.ts"
    },
    {
      "src": "/(.*)",
      "dest": "api/health.ts"
    }
  ]
}