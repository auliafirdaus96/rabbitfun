{
  "name": "rabbit-launchpad-backend",
  "version": "1.0.0",
  "description": "Backend API for Rabbit Launchpad",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "db:migrate": "npx prisma migrate dev",
    "db:generate": "npx prisma generate",
    "db:seed": "ts-node prisma/seed.ts",
    "db:studio": "npx prisma studio",
    "db:reset": "npx prisma migrate reset",
    "knex:migrate": "knex migrate:latest",
    "knex:rollback": "knex migrate:rollback",
    "knex:seed": "knex seed:run",
    "knex:make-migration": "knex migrate:make",
    "simple": "node simple-server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:unit": "jest tests/api/**/*.test.ts",
    "test:integration": "jest tests/integration/**/*.test.ts",
    "test:integration:real": "jest tests/integration/database-real.test.ts",
    "test:integration:mocked": "jest tests/integration/database-mock.test.ts",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:mocked": "jest tests/integration/database-mock.test.ts",
    "test:db": "jest tests/integration/database-real.test.ts",
    "test:db:setup": "chmod +x scripts/setup-test-db.sh && ./scripts/setup-test-db.sh",
    "test:db:cleanup": "chmod +x scripts/cleanup-test-db.sh && ./scripts/cleanup-test-db.sh",
    "test:db:seed": "ts-node prisma/seed-test.ts",
    "test:performance": "node scripts/performance-test.js",
    "test:load": "node scripts/performance-test.js load",
    "test:stress": "node scripts/performance-test.js stress",
    "test:spike": "node scripts/performance-test.js spike",
    "test:security": "node scripts/security-scan.js",
    "security:scan": "node scripts/security-scan.js all",
    "security:dependencies": "node scripts/security-scan.js dependencies",
    "security:code": "node scripts/security-scan.js code",
    "security:api": "node scripts/security-scan.js api",
    "security:files": "node scripts/security-scan.js files",
    "security:container": "node scripts/security-scan.js container",
    "security:auth": "node scripts/security-scan.js auth",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "lint:security": "eslint src/**/*.ts --config .eslintrc.js",
    "lint:perf": "eslint src/**/*.ts --max-warnings 0"
  },
  "dependencies": {
    "@prisma/client": "^6.16.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "axios": "^1.5.0",
    "bcrypt": "^5.1.1",
    "bull": "^4.11.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cron": "^2.4.4",
    "dotenv": "^16.6.1",
    "ethers": "^6.15.0",
    "express": "^4.18.2",
    "express-rate-limit": "^6.11.2",
    "express-validator": "^7.2.1",
    "helmet": "^7.0.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "module-alias": "^2.2.3",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.2",
    "pg": "^8.16.3",
    "prisma": "^6.16.3",
    "redis": "^4.6.8",
    "sharp": "^0.32.5",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "sqlite3": "^5.1.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@typechain/ethers-v6": "^0.5.1",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/cron": "^2.0.1",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.5",
    "@types/joi": "^17.2.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.5.0",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.10.2",
    "@types/serve-static": "^1.15.9",
    "@types/supertest": "^6.0.3",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.48.0",
    "jest": "^29.7.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typechain": "^8.3.2",
    "typescript": "^5.1.6"
  },
  "keywords": [
    "rabbit",
    "launchpad",
    "backend",
    "api",
    "postgresql",
    "blockchain",
    "bsc"
  ],
  "author": "Rabbit Launchpad Team",
  "license": "MIT"
}
